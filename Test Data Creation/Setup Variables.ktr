    <jsScripts>      <jsScript>        <jsScript_type>0</jsScript_type>
        <jsScript_name>Script 1</jsScript_name>
        <jsScript_script>&#x2f;&#x2f; Ignore header row in count&#xa;var csv_row_count &#x3d; csv_lines - 1&#x3b;&#xa;&#xa;&#x2f;&#x2f; Set csv_row_count&#xa;setVariable&#x28;&#x27;csv_row_count&#x27;, csv_lines, &#x27;r&#x27;&#x29;&#x3b;&#xa;&#xa;&#x2f;&#x2f; Set variables for different counts as ratio of csv_row_count&#xa;&#xa;&#x2f;&#x2f; How many contacts in the biggest household&#x3f;&#xa;var large_hh_count &#x3d; 100&#x3b;&#xa;if &#x28;csv_row_count &#x3c; 1000&#x29; &#x7b;&#xa;    large_hh_count &#x3d; ceil&#x28;csv_row_count &#x2a; .1&#x29;&#x3b;&#xa;&#x7d;&#xa;setVariable&#x28;&#x27;large_hh_count&#x27;, large_hh_count, &#x27;r&#x27;&#x29;&#x3b;&#xa;&#xa;&#x2f;&#x2f; How many contacts are in single member households&#x3f;&#xa;var single_hh_contacts &#x3d; null&#x3b;&#xa;if &#x28;csv_row_count &#x3c; 10000&#x29; &#x7b;&#xa;	single_hh_contacts &#x3d; ceil&#x28;csv_row_count &#x2a;.899&#x29;&#x3b;&#xa;&#x7d; else &#x7b;&#xa;	single_hh_contacts &#x3d; ceil&#x28;csv_row_count &#x2a;.97&#x29;&#x3b;&#xa;&#x7d;&#xa;&#xa;setVariable&#x28;&#x27;single_hh_contacts&#x27;, single_hh_contacts, &#x27;r&#x27;&#x29;&#x3b;&#xa;&#xa;&#x2f;&#x2f; How many contacts are directly linked to an Organization&#x3f;&#xa;org_only_contacts &#x3d; ceil&#x28;csv_row_count &#x2a;.001&#x29;&#x3b;&#xa;setVariable&#x28;&#x27;org_only_contacts&#x27;, org_only_contacts, &#x27;r&#x27;&#x29;&#x3b;&#xa;&#xa;&#x2f;&#x2f; Calculate number of households&#xa;var num_households &#x3d; ceil&#x28;&#x28;&#x28;csv_row_count - single_hh_contacts - org_only_contacts - large_hh_count &#x2b; 1&#x29; &#x2a; .5&#x29; &#x2b; single_hh_contacts&#x29;&#x3b;&#xa;&#xa;&#x2f;&#x2f; How many org opps&#x3f;&#xa;var orgs_with_opps_count &#x3d; ceil&#x28;&#x28;csv_row_count &#x2a; .5&#x29; &#x2a; .04&#x29;&#x3b;&#xa;var opps_per_org &#x3d; 10&#x3b;&#xa;setVariable&#x28;&#x27;orgs_with_opps_count&#x27;, orgs_with_opps_count, &#x27;r&#x27;&#x29;&#x3b;&#xa;setVariable&#x28;&#x27;opps_per_org&#x27;, opps_per_org, &#x27;r&#x27;&#x29;&#x3b;&#xa;&#xa;&#x2f;&#x2f; How many household opps&#x3f;&#xa;var hh_with_opps_count &#x3d; ceil&#x28;num_households &#x2a; .1&#x29;&#x3b;&#xa;var single_hh_with_opps_count &#x3d; hh_with_opps_count - 2&#x3b; &#x2f;&#x2f; leave room for 1 double and 1 large&#xa;var opps_per_hh &#x3d; 10&#x3b;&#xa;setVariable&#x28;&#x27;hh_with_opps_count&#x27;, hh_with_opps_count, &#x27;r&#x27;&#x29;&#x3b;&#xa;setVariable&#x28;&#x27;single_hh_with_opps_count&#x27;, single_hh_with_opps_count, &#x27;r&#x27;&#x29;&#x3b;&#xa;setVariable&#x28;&#x27;opps_per_hh&#x27;, opps_per_hh, &#x27;r&#x27;&#x29;&#x3b;&#xa;&#xa;&#x2f;&#x2f; How many households with many opps&#x3f;&#xa;var hh_with_many_opps_count &#x3d; ceil&#x28;num_households &#x2a; .004&#x29;&#x3b;&#xa;var opps_per_hh_with_many &#x3d; 100&#x3b;&#xa;setVariable&#x28;&#x27;hh_with_many_opps_count&#x27;, hh_with_many_opps_count, &#x27;r&#x27;&#x29;&#x3b;&#xa;setVariable&#x28;&#x27;opps_per_hh_with_many&#x27;, opps_per_hh_with_many, &#x27;r&#x27;&#x29;&#x3b;&#xa;&#xa;&#x2f;&#x2f; How many recurring donations&#x3f;&#xa;var recurring_donations_count &#x3d; ceil&#x28;csv_row_count &#x2a; .002&#x29;&#x3b;&#xa;var opps_per_recurring_donation &#x3d; 12&#x3b;&#xa;setVariable&#x28;&#x27;recurring_donations_count&#x27;, recurring_donations_count, &#x27;r&#x27;&#x29;&#x3b;&#xa;setVariable&#x28;&#x27;opps_per_recurring_donation&#x27;, opps_per_recurring_donation, &#x27;r&#x27;&#x29;&#x3b;&#xa;&#xa;&#x2f;&#x2f; How many soft credits&#x3f;&#xa;var opps_with_soft_credits &#x3d; ceil&#x28;csv_row_count &#x2a; .1&#x29;&#x3b;&#xa;var soft_credits_per_opp &#x3d; 10&#x3b;&#xa;var total_soft_credits &#x3d; opps_with_soft_credits &#x2a; soft_credits_per_opp&#x3b;&#xa;setVariable&#x28;&#x27;opps_with_soft_credits&#x27;, opps_with_soft_credits, &#x27;r&#x27;&#x29;&#x3b;&#xa;setVariable&#x28;&#x27;soft_credits_per_opp&#x27;, soft_credits_per_opp, &#x27;r&#x27;&#x29;&#x3b;&#xa;setVariable&#x28;&#x27;total_soft_credits&#x27;, total_soft_credits, &#x27;r&#x27;&#x29;&#x3b;&#xa;&#xa;&#x2f;&#x2f; How many opps with payments&#x3f;&#xa;var opps_with_payments &#x3d; ceil&#x28;csv_row_count &#x2a; .1&#x29;&#x3b;&#xa;var payments_per_opp &#x3d; 10&#x3b;&#xa;setVariable&#x28;&#x27;opps_with_payments&#x27;, opps_with_payments, &#x27;r&#x27;&#x29;&#x3b;&#xa;setVariable&#x28;&#x27;payments_per_opp&#x27;, payments_per_opp, &#x27;r&#x27;&#x29;&#x3b;&#xa;&#xa;&#x2f;&#x2f; How many households with extra addresses&#x3f;&#xa;var hh_with_extra_addresses &#x3d; ceil&#x28;num_households &#x2a; .005&#x29;&#x3b;&#xa;var extra_addresses_per_hh &#x3d; 100&#x3b;&#xa;var total_extra_addresses &#x3d; hh_with_extra_addresses &#x2a; extra_addresses_per_hh&#x3b;&#xa;setVariable&#x28;&#x27;hh_with_extra_addresses&#x27;, hh_with_extra_addresses, &#x27;r&#x27;&#x29;&#x3b;&#xa;setVariable&#x28;&#x27;extra_addresses_per_hh&#x27;, extra_addresses_per_hh, &#x27;r&#x27;&#x29;&#x3b;&#xa;setVariable&#x28;&#x27;total_extra_addresses&#x27;, total_extra_addresses, &#x27;r&#x27;&#x29;&#x3b;&#xa;&#xa;&#x2f;&#x2f; How many households with seasonal addresses&#x3f;&#xa;var hh_with_seasonal_addresses &#x3d; ceil&#x28;num_households &#x2a; .05&#x29;&#x3b;&#xa;var seasonal_addresses_per_hh &#x3d; 1&#x3b;&#xa;var total_seasonal_addresses &#x3d; hh_with_seasonal_addresses &#x2a; seasonal_addresses_per_hh&#x3b;&#xa;setVariable&#x28;&#x27;hh_with_seasonal_addresses&#x27;, hh_with_seasonal_addresses, &#x27;r&#x27;&#x29;&#x3b;&#xa;setVariable&#x28;&#x27;seasonal_addresses_per_hh&#x27;, seasonal_addresses_per_hh, &#x27;r&#x27;&#x29;&#x3b;&#xa;setVariable&#x28;&#x27;total_seasonal_addresses&#x27;, total_seasonal_addresses, &#x27;r&#x27;&#x29;&#x3b;&#xa;&#xa;&#x2f;&#x2f; How many GAU&#x27;s&#x3f;&#xa;var gau_count &#x3d; 1000&#x3b;&#xa;var opps_with_gau_allocations &#x3d; ceil&#x28;csv_row_count &#x2a; .1&#x29;&#x3b;&#xa;var gau_allocations_per_opp &#x3d; 10&#x3b;&#xa;&#xa;setVariable&#x28;&#x27;gau_count&#x27;, gau_count, &#x27;r&#x27;&#x29;&#x3b;&#xa;setVariable&#x28;&#x27;opps_with_allocations&#x27;, opps_with_gau_allocations, &#x27;r&#x27;&#x29;&#x3b;&#xa;setVariable&#x28;&#x27;allocations_per_opp&#x27;, gau_allocations_per_opp, &#x27;r&#x27;&#x29;&#x3b;&#xa;&#xa;&#x2f;&#x2f; How Many Campaigns with Allocations&#xa;var campaigns_with_allocations &#x3d; 10&#x3b;&#xa;var allocations_per_campaign &#x3d; 10&#x3b;&#xa;var members_per_campaign &#x3d; ceil&#x28;csv_row_count &#x2a; .05&#x29;&#x3b;&#xa;&#xa;setVariable&#x28;&#x27;campaigns_with_allocations&#x27;, campaigns_with_allocations, &#x27;r&#x27;&#x29;&#x3b;&#xa;setVariable&#x28;&#x27;allocations_per_campaign&#x27;, allocations_per_campaign, &#x27;r&#x27;&#x29;&#x3b;&#xa;setVariable&#x28;&#x27;members_per_campaign&#x27;, members_per_campaign, &#x27;r&#x27;&#x29;&#x3b;&#xa;&#xa;var total_addresses &#x3d; num_households &#x2b; total_seasonal_addresses &#x2b; total_extra_addresses&#xa;var total_opportunities &#x3d; &#x28;orgs_with_opps_count &#x2a; opps_per_org&#x29; &#x2b; &#x28;hh_with_opps_count &#x2a; opps_per_hh&#x29; &#x2b; &#x28;hh_with_many_opps_count &#x2a; opps_per_hh_with_many&#x29; &#x2b; &#x28;recurring_donations_count &#x2a; opps_per_recurring_donation&#x29;</jsScript_script>
